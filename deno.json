{
    "name": "evan-bot",
    "version": "1.0.0",
    "tasks": {
        "start": "deno run --allow-net --allow-env --allow-read --allow-write src/main.ts",
        "check": "deno check src/main.ts"
    },
    "imports": {
        "discord.js": "npm:discord.js@14",
        "mongodb": "npm:mongodb@6",
        "hono": "npm:hono@4",
        "@hono/node-server": "npm:@hono/node-server@1",
        "dotenv": "npm:dotenv@16",
        "crypto": "node:crypto"
    },
    "compilerOptions": {
        "strict": true,
        "lib": [
            "deno.ns",
            "dom"
        ]
    }
}